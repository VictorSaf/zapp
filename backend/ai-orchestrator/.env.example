# AI Orchestrator Service Configuration
NODE_ENV=development
SERVICE_NAME=ai-orchestrator
SERVICE_VERSION=1.0.0
PORT=3001

# Service Discovery (Consul)
CONSUL_HOST=localhost
CONSUL_PORT=8500
CONSUL_SERVICE_ID=ai-orchestrator-1
CONSUL_HEALTH_CHECK_INTERVAL=30s

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# Microservices URLs
API_SERVICE_URL=http://localhost:3000
FRONTEND_URL=http://localhost:5173

# gRPC Configuration
GRPC_PORT=50051
GRPC_MAX_MESSAGE_SIZE=4194304

# Queue Configuration
QUEUE_REDIS_HOST=localhost
QUEUE_REDIS_PORT=6379
QUEUE_REDIS_DB=1

# Monitoring
METRICS_PORT=9090
HEALTH_CHECK_PATH=/health
METRICS_PATH=/metrics

# Logging
LOG_LEVEL=info
LOG_FORMAT=json

# AI Configuration
AI_RESPONSE_TIMEOUT=30000
MAX_CONCURRENT_REQUESTS=10
AGENT_ORCHESTRATION_TIMEOUT=60000

# Load Balancing
LOAD_BALANCER_STRATEGY=round_robin
MAX_RETRIES=3
RETRY_DELAY=1000

# Security
JWT_SECRET=your-jwt-secret-here
CORS_ORIGINS=http://localhost:3000,http://localhost:5173

# Performance
REQUEST_TIMEOUT=30000
CONNECTION_POOL_SIZE=10
KEEP_ALIVE_TIMEOUT=65000